  "version": "1.0.0",
  "description": "Tin Skins' version of the SOEN 341 Project.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently -k --success=first --raw \"npm run server\" \"npm run client\"",
    "server": "npm --prefix backend run dev",
    "client": "npm --prefix frontend start",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint \"{backend,frontend}/**/*.{js,ts,jsx,tsx}\"",
    "lint:report": "eslint \"{backend,frontend}/**/*.{js,ts,jsx,tsx}\" -f table -o eslint-report.txt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/v1ctoriaD/Tin_Skins-SOEN341_Project_F25.git"
  },
