  },
  "homepage": "https://github.com/v1ctoriaD/Tin_Skins-SOEN341_Project_F25#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-formatter-table": "^7.32.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
